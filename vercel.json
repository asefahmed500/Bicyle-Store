{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node",
        "config": {
          "typescript": {
            "tsconfig": "tsconfig.json"
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/server.ts"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  